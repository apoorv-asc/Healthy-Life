  <body style="background-color: #003049;padding: 50px">
    <div class="container-scroller justify-content-center">
        <div class="row">
            <div class="col-lg-6">
                <img src="/assets/images/login.jpg" height="444px" width="550px">
            </div>
            <div class="col-lg-6">
                <div class="card " style="width: 25vw; padding: 20px;">
                    <div class="card-body">
                      <h2 class="card-title" style="font-size:40px; color:#EE5B5A;"><strong>Register User</strong></h2><br>
                      <form action="/registeration" method="post" class="forms-sample">
                        <div class="form-group">
                          <label for="exampleInputEmail1" style="font-size: 16px;">Email address</label>
                          <input name="username" type="email" class="form-control" id="exampleInputEmail1" placeholder="Email" />
                        </div>
                        <div class="form-group">
                          <label for="exampleInputPassword1">Password</label>
                          <input name="password" type="password" class="form-control" id="exampleInputPassword1" placeholder="Password" />
                        </div>
                        <div class="form-check form-check-flat form-check-primary">
                            <label class="form-check-label">
                              <input type="checkbox" class="form-check-input" /> Remember me </label>
                        </div>
                          
                        <button type="submit" class="btn btn-primary"> Submit </button>
                        <button class="btn btn-light" onclick="document.location.href='/login';">
                          <a href="/login">
                            Sign in
                          </a>
                        </button>
                      </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
  </body>
