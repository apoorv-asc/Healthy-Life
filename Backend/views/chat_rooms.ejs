  <body>
    <div class="container-scroller">
      <!-- partial:partials/_sidebar.html -->
      <%- include("partials/navbar") %>
        <!-- partial -->
        <div class="main-panel chat-page">
          <div class="content-wrapper pb-0">
            <div class="row">
              <div class="col-lg-12 grid-margin stretch-card chatcard">
                <main class="content">
                  <div class="container p-0">
                  <div class="card">
                    <div class="row g-0">
                      <div class="col-12 col-lg-5 col-xl-3 border-right" style="padding: 8px;">
                        <div class="m-0 text-muted"><strong>Groups</strong> 
                          <button type="button" class="btn btn-primary btn-md mr-1 px-3" data-target="#createGroupModal"  data-toggle="modal" style="margin-left:45%">
                          <i class="mdi mdi-account-plus"></i>
                        </button></div>

                        <!-- Groups of Logged in User -->
                        <hr>
                          <% if(rooms !=null) { %>
                          <% (rooms).forEach((room)=>{ %>
                              <a href="/user/chat/<%= room.id %>" class="list-group-item list-group-item-action border-0">
                                <div class="d-flex align-items-center py-1">
                                  <div class="position-relative">
                                    <img src="https://bootdey.com/img/Content/avatar/avatar3.png" class="rounded-circle mr-1" alt="Sharon Lessman" width="30" height="30">
                                  </div>
                                  <div class="flex-grow-0">
                                    <%= room.name %>
                                  </div>
                                </div>
                              </a>
                          <% }) } %>
                        <hr>

                        <div class="m-0 text-muted"><strong>Users</strong></div>
                        <hr>
                        <% if(prevChat!=null){ %>
                          <% (prevChat.Participants).forEach((participant)=>{ %>
                            <a href="#" class="list-group-item list-group-item-action border-0">
                              <div class="d-flex align-items-center py-1">
                                <div class="position-relative">
                                  <img src="https://bootdey.com/img/Content/avatar/avatar3.png" class="rounded-circle mr-1" alt="Sharon Lessman" width="30" height="30">
                                </div>
                                
                                <div class="flex-grow-0" class="position-relative">
                                  <%= participant %> &nbsp; &nbsp;
                                  <button onclick="document.location.href='/user/patient_info/apoorvsingh1120@gmail.com';" type="button" class="btn btn-success btn-rounded btn-icon btn-sm">
                                    <i class="mdi mdi-chart-line"></i>
                                  </button>
                                </div>
                              </div>
                            </a>
                          <% }) %>
                        <% } %>
                          
  
                          <!-- Chat Main content -->
                        <hr class="d-block d-lg-none mt-1 mb-0">
                      </div>
                      
                      <div class="col-12 col-lg-7 col-xl-9">
                        <div class="py-2 px-4 border-bottom d-none d-lg-block">
                          <div class="d-flex align-items-center py-1">
                            <div class="position-relative">
                              <img src="https://bootdey.com/img/Content/avatar/avatar3.png" class="rounded-circle mr-1" alt="Sharon Lessman" width="40" height="40">
                            </div>
                            <div class="flex-grow-1 pl-3">
                              <% if(prevChat != null){ %>
                              <strong><%= prevChat.ChatName %></strong>
                              <% } else{%>
                              <strong>Select a chat room</strong>
                              <% }%>
                            </div>
                            <div>
                              <!-- <button type="button" class="btn btn-success btn-md mr-1 px-3" data-toggle="modal" data-target=".bd-example-modal"></button> -->
                              <button type="button" class="btn btn-success btn-md mr-1 px-3" data-target="#addUsers"  data-toggle="modal">
                                <i class="mdi mdi-account-plus"></i>
                              </button>
                              <button type="button" class="btn btn-danger btn-md mr-1 px-3" data-target="#removeUsers"  data-toggle="modal">
                                <i class="mdi mdi-account-minus"></i>
                              </button>
                              <!-- <button class=""><i class="mdi mdi-account-plus"></i></button> -->
                            </div>
                          </div>
                        </div>
              
              
                        <div class="position-relative">
                          <div class="chat-messages p-4" style="height: 65vh;">
                            <div id="messages">
                              <% if(prevChat != null && prevChat.msg != null) { %>
                                <% (prevChat.msg).forEach((msg)=>{ %>
                                  <% if(msg.msg_type != "health"){ %>
                                    <% if(username == msg.username && msg.msg_type !="health" ){ %>
                                      <div class="chat-message-right pb-4 message" >
                                    <% }else if(msg.msg_type !="health" ){ %>
                                      <div class="chat-message-left pb-4">
                                    <% } %>
                                    
                                      <div>
                                        <img src="https://bootdey.com/img/Content/avatar/avatar1.png" class="rounded-circle mr-1" alt="Chris Wood" width="40" height="40">
                                        <div class="text-muted small text-nowrap mt-2"><%= msg.timestamp %></div>
                                      </div>
                                      <div class="flex-shrink-1 rounded py-2 px-3 mr-3">
                                        <div class="font-weight-bold mb-1"><%= msg.username %></div>
                                        <%= msg.message %>
                                      </div>  
                                    </div>
                                    <% if(msg.msg_type==="prs"){ %>
                                      <div class="chat-message-prescription pb-4">
                                        <div class="card-header font-weight-bold" style="font-size:14px">Prescription</div>
                                        <div class="rounded py-2 px-3 mr-3">
                                          <div class="mb-1"><%= msg.username%> prescribed you a medicine... Would you like to add this in your Daily Activity &nbsp;
                                            <a href="/ml/<%= msg.username %>/<%= msg.message %>" target="_blank">Yes</a>
                                            <a href="#">No</a>
                                          </div>
                                        </div>
                                      </div>
                                  <% }} else if(msg.msg_type==="prs"){ %>
                                    <div class="chat-message-prescription pb-4">
                                      <div class="card-header font-weight-bold" style="font-size:14px">Prescription</div>
                                      <div class="rounded py-2 px-3 mr-3">
                                        <div class="mb-1"><%= msg.username%> prescribed you a medicine... Would you like to add this in your Daily Activity &nbsp;
                                          <!-- <a href="/ml/<%= msg.username %>/<%= msg.message %>" target="_blank">Yes</a> -->
                                          <a href="/user/dashboard">Yes</a>
                                          <a href="#">No</a>
                                        </div>
                                      </div>
                                    </div>
                                  <% }else if(msg.msg_type==="health" && msg.symptoms[0] !=null){ %>
                                  <div class="chat-message-aidata pb-4">
                                    <div class="card-header font-weight-bold" style="font-size:14px">Symptom Checker Results</div>
                                    <div class="dark-color"><b>Symptoms</b></div>
                                    <div class="light-color">
                                      <ul>
                                        <% (msg.symptoms).forEach((sym)=>{ %>
                                          <li><%= sym  %></li>
                                        <% }) %>
                                      </ul>
                                    </div>
                                    <div class="dark-color"><b>Body Temperature</b></div>
                                    <div class="light-color"><%= msg.temp %> C</div>
                                    <div class="dark-color"><b>Duration</b></div>
                                    <div class="light-color"><%= msg.duration %> days</div>
                                    <div class="dark-color"><b>Severity</b></div>
                                    <div class="light-color"><%= msg.sev %></div>
                                    <div class="dark-color"><b>Predicted Diseases</b></div>
                                    <div class="light-color">
                                      <ul>
                                        <% for(var x=0;x<10; x=x+1){ %>
                                          <li><%= msg.diseases[x] %> - <%- parseFloat(msg.chances[x]).toFixed(1) %> %</li>
                                        <% } %>
                                      </ul>
                                    </div>
                                  </div>

                                  <% } %>
                                  
                              <% }) } %>
                            </div>
                          </div>
                        </div>
                        
                        <div class="flex-grow-0 py-3 px-4 border-top" style="background-color: #d4e6e7">
                          <form id="message-form">
                            <div class="input-group">
                              <select name="msg_type" class="form-control">
                                <option value="msg">Message</option>
                                <option value="prs">Prescription</option>
                              </select>
                            </div>
                            <div class="input-group">
                              <input name="message" type="text" class="form-control" placeholder="Type your message" style="height: 42px">
                              <button id="send-btn" class="btn btn-primary"><i class="mdi mdi-send"></i></button>
                            </div>
                          </form>
                        </div>
                        
                        <script id="message-template1" type="text/html">
                          <div class="message">
                              <div class="chat-message-right pb-4 message">
                              <div>
                                <img src="https://bootdey.com/img/Content/avatar/avatar1.png" class="rounded-circle mr-1" alt="Chris Wood" width="40" height="40">
                                <div class="text-muted small text-nowrap mt-2">{{createdAt}}</div>
                              </div>
                              <div class="flex-shrink-1 rounded py-2 px-3 mr-3">
                                <div class="font-weight-bold mb-1">{{username}}</div>
                                {{message}}
                              </div>
                            </div>
                          </div>
                        </script>

